    "@types/mocha": "^10.0.10",
    "@types/node": "^18.0.0",
    "mocha": "^11.7.1",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
