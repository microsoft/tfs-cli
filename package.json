{
  "name": "tfx-cli",
  "version": "0.21.5",
  "description": "CLI for Azure DevOps Services and Team Foundation Server",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/tfs-cli"
  },
  "main": "./_build/tfx-cli.js",
  "preferGlobal": true,
  "bin": {
    "tfx": "./_build/tfx-cli.js"
  },
  "scripts": {
    "clean": "rimraf _build && rimraf _tests",
    "build": "tsc -p .",
    "postbuild": "ncp app/tfx-cli.js _build/tfx-cli.js && ncp package.json _build/package.json && ncp app/exec/build/tasks/_resources _build/exec/build/tasks/_resources",
    "prepublishOnly": "npm run build",
    "build:tests": "tsc -p tsconfig.tests.json",
    "postbuild:tests": "ncp tests/build-samples _tests/build-samples && ncp tests/extension-samples _tests/extension-samples",
    "test": "npm run build:tests && mocha _tests/tests/**/*.js",
    "test:ci": "npm run build:tests && mocha _tests/tests/**/*.js --reporter xunit --reporter-options output=test-results.xml",
    "test:build-commands": "npm run build:tests && mocha _tests/tests/build-commands.js",
    "test:build-task-templates": "npm run build:tests && mocha _tests/tests/build-task-templates.js",
    "test:build-task-validation": "npm run build:tests && mocha _tests/tests/build-task-validation.js",
    "test:commandline": "npm run build:tests && mocha _tests/tests/commandline.js",
    "test:extension-commands": "npm run build:tests && mocha _tests/tests/extension-commands.js",
    "test:extension-advanced": "npm run build:tests && mocha _tests/tests/extension-advanced.js",
    "test:extension-complex": "npm run build:tests && mocha _tests/tests/extension-complex.js",
    "test:extension-task-validation": "npm run build:tests && mocha _tests/tests/extension-task-validation.js"
  },
  "dependencies": {
    "archiver": "2.0.3",
    "azure-devops-node-api": "^14.0.0",
    "clipboardy": "^4.0.0",
    "colors": "~1.3.0",
    "glob": "7.1.2",
    "jju": "^1.4.0",
    "json-in-place": "^1.0.1",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "onecolor": "^2.5.0",
    "os-homedir": "^1.0.1",
    "prompt": "^1.3.0",
    "shelljs": "^0.8.5",
    "util.promisify": "^1.0.0",
    "uuid": "^3.0.1",
    "validator": "^13.7.0",
    "winreg": "0.0.12",
    "xml2js": "^0.5.0"
  },
  "devDependencies": {
    "@types/clipboardy": "~1.1.0",
    "@types/glob": "^5.0.29",
    "@types/jju": "^1.4.1",
    "@types/jszip": "~3.1.2",
    "@types/lodash": "~4.14.110",
    "@types/mkdirp": "^1.0.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "8.10.66",
    "@types/shelljs": "^0.8.11",
    "@types/uuid": "^2.0.29",
    "@types/validator": "^4.5.27",
    "@types/winreg": "^1.2.29",
    "@types/xml2js": "0.0.27",
    "mocha": "^11.7.1",
    "ncp": "^2.0.0",
    "rimraf": "^2.6.1",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "author": "Microsoft Corporation",
  "contributors": [
    {
      "email": "trgau@microsoft.com",
      "name": "Trevor Gau"
    }
  ],
  "license": "MIT"
}
